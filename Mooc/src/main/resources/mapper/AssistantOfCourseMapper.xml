<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mooc.mooc.mapper.AssistantOfCourseMapper">
  <resultMap id="BaseResultMap" type="com.mooc.mooc.model.AssistantOfCourse">
    <id column="course_id" jdbcType="INTEGER" property="courseId" />
    <id column="assistant_id" jdbcType="INTEGER" property="assistantId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="map">
    delete from assistant_of_course
    where course_id = #{courseId,jdbcType=INTEGER}
      and assistant_id = #{assistantId,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.mooc.mooc.model.AssistantOfCourse">
    insert into assistant_of_course (course_id, assistant_id)
    values (#{courseId,jdbcType=INTEGER}, #{assistantId,jdbcType=INTEGER})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select course_id, assistant_id
    from assistant_of_course
  </select>
</mapper>